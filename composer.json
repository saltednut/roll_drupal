{
    "name": "saltednut/roll_drupal",
    "type": "drupal-profile",
    "description": "Profile templates for the Roll project",
    "homepage": "https://www.dianawynn.com/roll",
    "license": "GPL-2.0-or-later",
    "require": {
        "bower-asset/imagesloaded": "^4.1",
        "bower-asset/masonry-layout": "^4.2",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/admin_toolbar": "^3.4",
        "drupal/coffee": "^1.3",
        "drupal/config_filter": "^2.4",
        "drupal/config_ignore": "^2.4",
        "drupal/config_partial_export": "^2.0@alpha",
        "drupal/config_readonly": "^1.0@beta",
        "drupal/config_rewrite": "^1.5",
        "drupal/config_split": "^2.0@RC",
        "drupal/content_access": "^2.0",
        "drupal/core": "^10.1.4",
        "drupal/core-composer-scaffold": "^9",
        "drupal/core-project-message": "^9",
        "drupal/devel": "^5.1",
        "drupal/eu_cookie_compliance": "^1.24",
        "drupal/file_browser": "^1.4",
        "drupal/google_tag": "^2.0",
        "drupal/menu_link_attributes": "^1.3",
        "drupal/metatag": "^2.0",
        "drupal/next": "^1.6",
        "drupal/pathauto": "^1.11",
        "drupal/readonlymode": "^1.2",
        "drupal/shield": "^1.7",
        "drupal/simple_sitemap": "^4.1",
        "drupal/stage_file_proxy": "^2.1",
        "drupal/token": "^1.12",
        "drupal/tome": "^1.11",
        "drupal/webform": "^6.2@beta",
        "drupal/youtube": "^2.0",
        "drush/drush": "^12.2",
        "oomphinc/composer-installers-extender": "^2.0"
    },
    "config": {
        "preferred-install": {
            "drupal/core": "dist"
        },
        "process-timeout": 0,
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "oomphinc/composer-installers-extender": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "docroot/"
            }
        },
        "installer-types": [
            "npm-asset",
            "bower-asset"
        ],
        "enable-patching": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": {
            }
        },
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "docroot/libraries/slick": ["bower-asset/slick-carousel"],
            "docroot/libraries/masonry": ["bower-asset/masonry-layout"],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ]
        }
    },
    "require-dev": {
        "drupal/core-dev": "^10"
    },
    "scripts": {
        "post-install-cmd": [
            "rm -rf docroot/profiles/roll",
            "mkdir docroot/profiles/roll",
            "cp roll.info.yml docroot/profiles/roll/.",
            "cp -R config docroot/profiles/roll/.",
            "cp -R profiles docroot/profiles/roll/."
        ]
    }
}
