{
    "name": "saltednut/roll_drupal",
    "type": "drupal-profile",
    "description": "Profile templates for the Roll project",
    "homepage": "https://www.dianawynn.com/roll",
    "license": "GPL-2.0-or-later",
    "require": {
        "acquia/cohesion": "^6.6",
        "bower-asset/blazy": "^1.8",
        "bower-asset/imagesloaded": "^4.1",
        "bower-asset/masonry-layout": "^4.2",
        "bower-asset/slick-carousel": "^1.8",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/acquia_search": "^3.0",
        "drupal/addtoany": "^1.15",
        "drupal/admin_toolbar": "^3.0",
        "drupal/asset_injector": "^2.7",
        "drupal/coffee": "^1.2",
        "drupal/config_filter": "^2.2",
        "drupal/config_ignore": "^2.3",
        "drupal/config_partial_export": "^1.6",
        "drupal/config_readonly": "^1.0@beta",
        "drupal/config_rewrite": "^1.4",
        "drupal/config_split": "^1.4",
        "drupal/content_access": "1.x-dev",
        "drupal/core": "^9.3",
        "drupal/core-composer-scaffold": "^9",
        "drupal/core-project-message": "^9",
        "drupal/default_content": "^2.0@alpha",
        "drupal/devel": "^4.1",
        "drupal/domain": "1.x-dev",
        "drupal/entity_browser": "^2.6",
        "drupal/entity_embed": "^1.1",
        "drupal/facebook_pixel": "^1.0.0-alpha5",
        "drupal/facets": "^1.8",
        "drupal/field_group": "^3.2",
        "drupal/file_browser": "^1.3",
        "drupal/focal_point": "^1.5",
        "drupal/google_tag": "^1.4",
        "drupal/inline_responsive_images": "^2.2",
        "drupal/lazy": "^3.10",
        "drupal/m4032404": "^1.0@alpha",
        "drupal/memcache": "^2.3",
        "drupal/menu_link_attributes": "^1.2",
        "drupal/migrate_plus": "^5.1",
        "drupal/paragraphs": "^1.12",
        "drupal/paragraphs_ee": "^1.8",
        "drupal/paragraphs_sets": "^2.8",
        "drupal/pathauto": "^1.8",
        "drupal/readonlymode": "^1.1",
        "drupal/redirect": "^1.6",
        "drupal/robotstxt": "^1.4",
        "drupal/search_api": "^1.20",
        "drupal/search_api_solr": "^4.2",
        "drupal/search_autocomplete": "^2.0",
        "drupal/shield": "1.x-dev",
        "drupal/simple_sitemap": "^3.10",
        "drupal/stage_file_proxy": "^1.1",
        "drupal/tinypng": "^1.1",
        "drupal/token": "^1.9",
        "drupal/typed_data": "^1.0-alpha5",
        "drupal/views_data_export": "^1.0",
        "drupal/views_infinite_scroll": "^1.8",
        "drupal/views_slideshow": "^4.8",
        "drupal/webform": "^6.0",
        "drupal/yoast_seo": "^1.7",
        "drupal/youtube": "^1.2",
        "drush/drush": "^10.5",
        "jquery/cycle": "^3.0",
        "jquery/hoverintent": "^1.9",
        "json/json2": "^2.0",
        "library-enyo/dropzone": "^4.3",
        "oomphinc/composer-installers-extender": "^2.0"
    },
    "config": {
        "preferred-install": {
            "drupal/core": "dist"
        },
        "process-timeout": 0,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "library-enyo/dropzone",
                "version": "4.3.0",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/enyo/dropzone",
                    "type": "git",
                    "reference": "v4.3.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "jquery/cycle",
                "version": "3.0.3",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "jquery.cycle"
                },
                "dist": {
                    "url": "https://github.com/malsup/cycle/archive/refs/tags/3.0.3.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "jquery/hoverintent",
                "version": "1.9.0",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "jquery.hoverIntent"
                },
                "dist": {
                    "url": "https://github.com/briancherne/jquery-hoverIntent/archive/refs/tags/v1.9.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "json/json2",
                "version": "2.0",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "json2"
                },
                "dist": {
                    "url": "https://github.com/douglascrockford/JSON-js/archive/refs/heads/master.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "docroot/"
            }
        },
        "installer-types": [
            "npm-asset",
            "bower-asset"
        ],
        "enable-patching": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": {
                "Allow profiles to define a base/parent profile": "https://www.drupal.org/files/issues/2021-11-18/1356276-675.patch",
                "It is possible to overflow the number of items allowed in Media Library": "https://www.drupal.org/files/issues/2019-12-28/3082690-80.patch",
                "Media Library widget produces error this value should not be null when field is required": "https://www.drupal.org/files/issues/2020-10-08/3160238-25.patch",
                "SQLite database locking errors cause fatal errors": "https://www.drupal.org/files/issues/1120020-59.patch"
            },
            "drupal/default_content": {
                "2698425 - Duplicate content issues in default content": "https://git.drupalcode.org/project/default_content/-/merge_requests/5.patch"
            }
        },
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "docroot/libraries/slick": ["bower-asset/slick-carousel"],
            "docroot/libraries/masonry": ["bower-asset/masonry-layout"],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ]
        }
    },
    "require-dev": {
        "drupal/core-dev": "^9"
    },
    "scripts": {
        "post-install-cmd": [
            "rm -rf docroot/profiles/roll",
            "mkdir docroot/profiles/roll",
            "cp roll.info.yml docroot/profiles/roll/.",
            "cp -R config docroot/profiles/roll/.",
            "cp -R profiles docroot/profiles/roll/."
        ]
    }
}
